{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build_cmake_jack",
      "type": "shell",
      "command": "cmake",
      "args": [
        "-B${workspaceFolder}/build/release_jack",
        "-DBUILD_FOR_JACK=1"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
    },
    {
      "label": "build_make_jack",
      "type": "shell",
      "command": "make",
      "args": ["-C", "${workspaceFolder}/build/release_jack"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "dependsOn": ["build_cmake_jack"],
    },
    {
      "label": "build_cmake",
      "type": "shell",
      "command": "cmake",
      "args": [
        "-B${workspaceFolder}/build/release/"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
    },
    {
      "label": "build_make",
      "type": "shell",
      "command": "make",
      "args": ["-C", "${workspaceFolder}/build/release"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "dependsOn": ["build_cmake"],
    }
  ]
}
